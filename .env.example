# Frontend Environment Variables
# Copy to .env in the root directory

# Hosted backend API URL (used on Vercel/production)
VITE_API_URL=https://budgetbuddy-prod.up.railway.app

# Local backend API URL (used when app runs on localhost)
VITE_API_URL_LOCAL=http://localhost:8000

# Gemini API Key (for direct frontend usage if needed)
VITE_GEMINI_API_KEY=your_gemini_api_key_here
